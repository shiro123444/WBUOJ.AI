# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/ai_club_oj?schema=public"

# Redis
REDIS_URL="redis://localhost:6379"

# JWT
JWT_SECRET="your-super-secret-jwt-key-change-in-production"
JWT_EXPIRES_IN="7d"

# Server
PORT=3000
NODE_ENV=development

# AI Service (Optional)
OPENAI_API_KEY=""
AI_DAILY_LIMIT=20

# OAuth (Optional)
GITHUB_CLIENT_ID=""
GITHUB_CLIENT_SECRET=""
WECHAT_APP_ID=""
WECHAT_APP_SECRET=""

# Judge (Hydro Judge)
JUDGE_SECRET="your-judge-secret-key-change-in-production"
JUDGE_TEST_DATA_PATH="./test-data"
JUDGE_DEFAULT_TIME_LIMIT=1000
JUDGE_DEFAULT_MEMORY_LIMIT=256
# Judge 服务器 URL (评测机用于下载文件的地址)
# 如果评测机和后端在同一台机器，使用 http://localhost:3000
# 如果评测机在远程机器，使用后端服务器的公网地址
JUDGE_SERVER_URL="http://localhost:3000"
